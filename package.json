{
  "name": "json-editor",
  "version": "1.0.0",
  "description": "Json格式化",
  "main": "main.js",
  "build": {
    "appId": "com.json-editor.app",
    "mac": {
      "icon": "images/icon.png",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "images/icon.png",
      "target": [
        "nsis",
        "zip"
      ]
    }
  },
  "scripts": {
    "start": "electron .",
    "dist-win": "electron-builder --win --x64",
    "dist-darwin": "electron-builder --mac --x64"
  },
  "author": "xianghu",
  "license": "ISC",
  "devDependencies": {
    "electron": "^11.5.0",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "jsoneditor": "^7.0.4",
    "moment": "^2.24.0"
  }
}
